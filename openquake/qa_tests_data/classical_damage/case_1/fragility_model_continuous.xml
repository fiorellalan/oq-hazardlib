<?xml version="1.0" encoding="utf-8"?>
<nrml xmlns="http://openquake.org/xmlns/nrml/0.4" 
	  xmlns:gml="http://www.opengis.net/gml">
    <fragilityModel format="continuous">
        <description>
            Fragility model for Nepal
        </description>
        <limitStates>
            slight moderate extensive complete
        </limitStates>
        <ffs noDamageLimit="0.05" type="lognormal">
            <taxonomy>W</taxonomy>
            <IML IMT="PGA" imlUnit="g" maxIML="1.4" minIML="0.05"/>
            <ffc ls="slight">
                <params mean="0.160" stddev="0.104"/>
            </ffc>
            <ffc ls="moderate">
                <params mean="0.225" stddev="0.158"/>
            </ffc>
            <ffc ls="extensive">
                <params mean="0.400" stddev="0.300"/>
            </ffc>
            <ffc ls="complete">
                <params mean="0.600" stddev="0.48"/>
            </ffc>
        </ffs>
    </fragilityModel>
</nrml>
