<?xml version='1.0' encoding='UTF-8'?>
<nrml xmlns:gml="http://www.opengis.net/gml" 
      xmlns="http://openquake.org/xmlns/nrml/0.4">

<fragilityModel format="continuous">
  <description>cont ff | tax1 | zero miniml</description>
  <limitStates>ds1 ds2 ds3 ds4</limitStates>
  <ffs type="lognormal">
    <taxonomy>tax1</taxonomy>
    <IML IMT="PGA" imlUnit="g" minIML="0.0" maxIML="5.0"/>
    <ffc ls="ds1">
      <params mean="0.30" stddev="0.73"/>
    </ffc>
    <ffc ls="ds2">
      <params mean="0.60" stddev="0.68"/>
    </ffc>
    <ffc ls="ds3">
      <params mean="1.20" stddev="0.67"/>
    </ffc>
    <ffc ls="ds4">
      <params mean="2.40" stddev="0.65"/>
    </ffc>
  </ffs>
</fragilityModel>

</nrml>